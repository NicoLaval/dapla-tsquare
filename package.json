{
  "name": "dapla-tsquare",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@statisticsnorway/dapla-js-utilities": "^0.1.3",
    "axios": "^0.20.0",
    "axios-hooks": "^2.1.0",
    "moment": "^2.29.0",
    "react": "^16.13.1",
    "react-axios": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-moment": "^1.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^1.1.0"
  },
  "devDependencies": {
    "@beam-australia/react-env": "^2.1.0",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^11.0.0",
    "@testing-library/user-event": "^12.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-env --dest ./build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prestart": "react-env --dest ./public",
    "coverage": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/index.js",
      "!src/**/index.js",
      "!src/configurations/**",
      "!src/enums/**",
      "!src/__tests__/test-data/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "coverageReporters": [
      "cobertura",
      "lcov",
      "text"
    ]
  }
}
